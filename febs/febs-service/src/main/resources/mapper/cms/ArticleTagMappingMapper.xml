<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.febs.cms.dao.ArticleTagMappingMapper">
    <select id="queryArticleCounts"  resultType="java.lang.Integer">
        SELECT count(id) FROM t_article_tag_mapping where tag_id=#{tagId}
    </select> 
</mapper>